{
  "apiVersion": "argoproj.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "airflow",
    "labels": {
      "name": "airflow"
    }
  },
  "spec": {
    "project": "default",
    "source": {
      "helm": {
        "version": "v3",
        "values": "{\"dags\":{\"gitSync\":{\"enabled\":true,\"repo\":\"https://github.com/polyseam/demo-dag-bag\",\"branch\":\"main\",\"wait\":70}},\"createUserJob\":{\"useHelmHooks\":false},\"migrateDatabaseJob\":{\"useHelmHooks\":false}}"
      },
      "repoURL": "https://airflow.apache.org",
      "targetRevision": "1.6.0",
      "chart": "airflow"
    },
    "destination": {
      "server": "https://kubernetes.default.svc",
      "namespace": "airflow"
    },
    "syncPolicy": {
      "automated": {
        "prune": true,
        "selfHeal": true,
        "allowEmpty": false
      },
      "syncOptions": [
        "Validate=false",
        "CreateNamespace=true",
        "PrunePropagationPolicy=foreground",
        "PruneLast=true"
      ],
      "retry": {
        "limit": 10,
        "backoff": {
          "duration": "5s",
          "factor": 2,
          "maxDuration": "4m"
        }
      }
    }
  }
}